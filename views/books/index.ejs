<form action="/books" method="GET">
    <div>
        <label>Title</label>
        <input type="text" name="title" value="<%= queryOption.title %> ">
    </div>
    <div>
        <label>Published After</label>
        <input type="date" name="publishedAfter" value="<%= queryOption.publishedAfter %>">
    </div>
    <div>
        <label>Published Before</label>
        <input type="date" name="publishedBefore" value="<%= queryOption.publishedBefore %>">
    </div>

    <button type="submit">Search</button>
</form>
<div>
    <% books.forEach(item => { %>
    <a href="/books/<%= item.id  %>">

        <img height="200" width='150' src="<%= item.coverImagePath %> ">
    </a>
    <% }) %>

</div>